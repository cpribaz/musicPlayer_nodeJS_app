<h2>Search Artists</h2>
<form action="/artists" method="GET">
    <label>Name</label>
    <input type="text" name="artist_name" value="<% searchOptions.artist_name %>">
    <button type="submit">Search</button>
</form>
<br>
<% artists.forEach(artist => { %>
    <div><%= artist.artist_name %></div>
<% }) %>